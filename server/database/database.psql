CREATE TABLE IF NOT EXISTS table_question(
    question_id SERIAL PRIMARY KEY,
    question VARCHAR(500),
    image VARCHAR(100),
    subject VARCHAR(50),
    time int

);

CREATE TABLE IF NOT EXISTS table_answer(
    answer_id SERIAL PRIMARY KEY,
    question_id int NOT NULL,
